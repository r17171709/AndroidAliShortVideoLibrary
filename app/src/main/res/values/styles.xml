    <style name="BeautyLevelBtnStyle">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/alivc_beauty_default_bt_selector</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
    </style>

    <style name="NextBtnStyle">
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">@dimen/alivc_common_font_12</item>
        <item name="android:background">@drawable/alivc_svideo_bg_next</item>
        <item name="android:layout_height">27dp</item>
        <item name="android:layout_width">@dimen/alivc_common_width_tv_60</item>
        <item name="android:text">@string/alivc_base_svideo_next</item>
    </style>

    <style name="alivc_record_line_style" parent="@android:style/Widget.EditText">

        <item name="android:maxLines">2</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorHint">@android:color/darker_gray</item>
        <item name="android:background">@drawable/alivc_record_round_bkg</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
    </style>

    <style name="alivc_record_btn_style" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/alivc_record_btn_bg</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_height">36dp</item>
    </style>

    <declare-styleable name="MusicCircleProgressBar">
        <attr name="musicBackgroundWidth" format="dimension" />
        <attr name="musicBackgroundHeight" format="dimension" />
        <attr name="musicProgressWidth" format="dimension" />
        <attr name="musicProgressThickness" format="dimension" />
        <attr name="musicBackgroundColor" format="color|reference" />
        <attr name="musicProgressColor" format="color|reference" />
        <attr name="musicProgressThicknessColor" format="color|reference" />
    </declare-styleable>

    <style name="alivc_crop_line_style" parent="@android:style/Widget.EditText">

        <item name="android:maxLines">2</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@color/alivc_common_font_white</item>
        <item name="android:textColorHint">@color/alivc_common_font_gray_darker</item>
        <item name="android:background">@drawable/alivc_crop_round_bkg</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
    </style>

    <style name="alivc_crop_btn_style">
        <item name="android:background">@drawable/alivc_crop_btn_bg</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
        <item name="android:textColor">@color/alivc_common_font_white</item>
        <item name="android:layout_height">36dp</item>
    </style>


    <style name="Crop_MySwitchTheme">
        <item name="colorControlActivated">@color/alivc_common_bg_cyan_light</item>
        <item name="colorSwitchThumbNormal">@color/alivc_common_bg_white_bright</item>
        <item name="android:colorForeground">@color/alivc_common_bg_white_alpha_10</item>
    </style>

    <style name="edit_bottom_dialog_animation">
        <item name="android:windowEnterAnimation">@anim/aliyun_svideo_anim_trans_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/aliyun_svideo_anim_layout_bottom_out</item>
    </style>

    <style name="QUDemoBottomStyle" parent="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <style name="TextDlgStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="AnimationDlgStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="ResourcePreviewStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="alivc_svideo_effect_tab">
        <item name="android:paddingLeft">@dimen/alivc_common_padding_20</item>
        <item name="android:paddingRight">@dimen/alivc_common_padding_20</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/alivc_common_font_11</item>
        <item name="android:drawablePadding">@dimen/alivc_svideo_tab_tv_iv_distance</item>
    </style>
</resources>